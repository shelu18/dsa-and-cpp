#include <iostream>
#include<string>
using namespace std;

bool validstring(string str){

for(int i=0;i<str.length(); i++){
    if(str[0]=='a'&& str[i+1]=='a'&&str[i+2]=='a'){
        return 1;
    }
    else{
        return 0;
    }
}
}
int main(){
    string s;
    cout<<"enter string";
    cin>>s;
    if(validstring(s)){
        cout<<"sahi he";
    }
    else{
        cout<<"galat he";
    }
    return 0;
}

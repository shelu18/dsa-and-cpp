# dsa-and-cpp
#include<iostream>
using namespace std;
int main(){

int a,b;
cout<<"enter a number a";
cin>>a;


cout<<"enter a number b";
cin>>b;

if(a>b)

    cout<<"a is greater number";

    if(b>a)


        cout<<"b is greater number";
    
        else
        
        cout<<"both numbers are equal";
    return 0;


}











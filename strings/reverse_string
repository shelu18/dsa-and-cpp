#include<iostream> 
#include<string>
using namespace std;
 void reverse(string& str){
     int end =str.length()-1;
     int start= 0;
     while (start<end){
         swap(str[start],str[end]);
         start++;
        
         end--;
     }
     }
 
int main(){
string name;
cout<<"enter the value of string"<<endl;
cin>>name;
reverse(name);
cout<<name;
}
